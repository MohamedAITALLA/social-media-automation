{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "crons": [
    {
      "path": "/api/cron.py",
      "schedule": "0 * * * *"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
} 